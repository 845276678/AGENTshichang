# 生产环境变量配置
# 基础配置
NODE_ENV=production
NEXTAUTH_URL=https://yourdomain.com
NEXTAUTH_SECRET=your-super-secret-key-here

# 数据库配置
DATABASE_URL=postgresql://username:password@host:5432/database
POSTGRES_DB=ai_marketplace
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-strong-password
POSTGRES_REPLICATION_PASSWORD=replication-password

# JWT和认证
JWT_SECRET=your-jwt-secret-key
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# AI服务配置 (仅保留已配置的3个服务)
DEEPSEEK_API_KEY=your-deepseek-api-key
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DEEPSEEK_MODEL=deepseek-chat
DASHSCOPE_API_KEY=your-dashscope-api-key
ZHIPU_API_KEY=your-zhipu-api-key

# 存储配置
ALIYUN_OSS_REGION=oss-cn-hangzhou
ALIYUN_OSS_ACCESS_KEY_ID=your-oss-access-key-id
ALIYUN_OSS_ACCESS_KEY_SECRET=your-oss-access-key-secret
ALIYUN_OSS_BUCKET=your-oss-bucket-name

# Redis配置
REDIS_URL=redis://redis:6379
REDIS_PASSWORD=your-redis-password

# 监控配置
GRAFANA_ADMIN_PASSWORD=your-grafana-password
GRAFANA_DOMAIN=grafana.yourdomain.com

# Docker镜像
DOCKER_IMAGE=ghcr.io/yourusername/ai-marketplace:latest